<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <context-param>
    <param-name>defaultNoItems</param-name>
    <param-value>0</param-value>
  </context-param>

  <context-param>
    <param-name>defaultPrice</param-name>
    <param-value>0</param-value>
  </context-param>

  <context-param>
    <param-name>defaultProvince</param-name>
    <param-value>Ontario</param-value>
  </context-param>

  <context-param>
    <param-name>defaultTaxRate</param-name>
    <param-value>13.0</param-value>
  </context-param>

  <welcome-file-list>
    <welcome-file>Pricing.html</welcome-file>
  </welcome-file-list>
</web-app>
